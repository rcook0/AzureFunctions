{
  "IsEncrypted": false,
  "Values": {
    // Core runtime settings
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "node",

    // Cosmos DB
    "CosmosDBConnection": "AccountEndpoint=https://localhost:8081/;AccountKey=your-key;",

    // Azure Service Bus
    "ServiceBusConnection": "Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key",

    // Event Hubs
    "EventHubConnection": "Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key",

    // Azure SignalR Service
    "AzureSignalRConnectionString": "Endpoint=https://<yoursignalr>.service.signalr.net;AccessKey=your-key;Version=1.0;",

    // Application Insights (optional for telemetry)
    "APPINSIGHTS_INSTRUMENTATIONKEY": "00000000-0000-0000-0000-000000000000"
  }
}